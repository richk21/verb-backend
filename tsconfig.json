{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "./src",
    "outDir": "./dist",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "strict": true,
    "types":["node"],
    "typeRoots": ["./src/types", "./node_modules/@types"]
  },
  "include": ["src"],
}
